{
    "name": "gpupo/petfinder",
    "description": "PHP Sphinx faceted search over Official Sphinx searchd client (PHP API) with Oriented Object results based",
    "keywords": ["sphinx", "faceted", "search", "API", "client", "facet"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "gpupo/common": "~1.3",
        "psr/log": "~1.0",
        "zendframework/zend-validator": "2.*",
        "zendframework/zend-filter": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\Petfinder\\": "src",
            "Gpupo\\Tests\\Petfinder\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "minimum-stability" : "stable"
}
